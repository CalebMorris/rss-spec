{
  "name": "rss-spec",
  "version": "0.0.1",
  "description": "RSS feed and item specification",
  "main": "./dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel -d dist src/",
    "lint": "./node_modules/.bin/eslint src test *.js",
    "flow": "./node_modules/.bin/flow check",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/mocha --compilers js:babel-register --recursive",
    "prepublish": "npm run build"
  },
  "keywords": [
    "RSS",
    "xml",
    "feed",
    "spec",
    "es6"
  ],
  "author": "Caleb Morris <caleb.morris.g@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CalebMorris/rss-spec"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^3.0.1",
    "eslint-config-honu-base": "^0.3.0",
    "eslint-plugin-flowtype": "^2.3.0",
    "flow-bin": "^0.29.0",
    "mocha": "^2.5.3"
  },
  "dependencies": {
    "iflow-moment": "^1.1.13"
  }
}
